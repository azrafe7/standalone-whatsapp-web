{
  "name": "Standalone WhatsApp Web",
  "version": "0.1.1",
  "description": "Open WhatsApp Web in a standalone popup window",
  "manifest_version": 3,
  "permissions": [],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "assets/icon128.png"
  },
  "action": {
    "default_icon": {

      "128": "assets/icon128.png"
    },
    "default_title": "Standalone WhatsApp Web"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+W"
      }
    }
  }
}